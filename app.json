{
  "expo": {
    "name": "Track Task Flow",
    "slug": "tracktaskflow",
    "version": "1.0.2",
    "orientation": "default",
    "icon": "./assets/images/Logo.png",
    "scheme": "tracktaskflow",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "NSAdvertisingAttributionReportEndpoint": "https://appsflyer-skadnetwork.com/",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "eqhxz8m8av.skadnetwork"
          }
        ]
      },
      "bundleIdentifier": "com.tracktaskflow",
      "associatedDomains": [
        "applinks:tracktaskflow.onelink.me"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ],
      "package": "com.tracktaskflow"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-appsflyer",
        {
          "shouldUseStrictMode": false
        }
      ],
      [
        "onesignal-expo-plugin",
        {
          "mode": "production"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ],
    "runtimeVersion": "1.0.2",
    "updates": {
      "requestHeaders": {
        "expo-channel-name": "tracktaskflow"
      },
      "url": "https://u.expo.dev/d704a94a-b58c-4685-aea2-28663368b14f"
    },
    "extra": {
      "eas": {
        "projectId": "d704a94a-b58c-4685-aea2-28663368b14f"
      }
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
